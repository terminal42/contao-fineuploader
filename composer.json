{
    "name": "terminal42/contao-fineuploader",
    "description": "Uploader extension with chunking support for Contao Open Source CMS using FineUploader by Widen",
    "keywords": [
        "contao",
        "uploader",
        "fineuploader",
        "chunking"
    ],
    "type": "contao-module",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "terminal42 gmbh",
            "homepage": "http://terminal42.ch"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "contao/core-bundle": "^4.4",
        "codefog/contao-haste": "~4.17"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "phpunit/phpunit": "5.5.*"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "Terminal42\\FineUploaderBundle\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "Terminal42\\FineUploaderBundle\\ContaoManager\\Plugin"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
